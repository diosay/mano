<?xml version="1.0" encoding="UTF-8"?>
<application>
    <settings>
        <property name="application.name">default application</property>
        <property name="application.host">*</property>
        <property name="application.vpath">/</property>
        <property name="application.class">mano.web.WebAppcation</property>
        <property name="application.logger.category">{application.name}</property>
        <property name="application.public.data.path">{mano.dir}/AppData/{application.name}/public</property>
        <property name="application.private.data.path">{mano.dir}/AppData/{application.name}/private</property>
    </settings>
    
</application>
