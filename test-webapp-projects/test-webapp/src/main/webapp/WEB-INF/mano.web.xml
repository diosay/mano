    <!--TODO:日志-->
    <logging enable="true" logger="service:logging.service|class:logger.providerImpl">
        <level value="100" path="~/log/{yyyyMMdd}.log"/>;
    </logging>
    <!--其它配置 请见文档-->
</application>
